{
  "preferGlobal": true,
  "main": "index",
  "name": "migraticon",
  "description": "A simple CLI tool to migrate hipchat emoticons to slack.",
  "version": "0.0.1",
  "author": "Daniel Bohannon <tkdan235@gmail.com>",
  "engines": {
    "node": ">=0.10.0"
  },
  "directories": {
    "bin": "bin",
    "lib": "lib"
  },
  "keywords": [
    "slack"
  ],
  "dependencies": {
    "async": "^1.5.2",
    "coffee-script": "^1.10.0",
    "colors": "^1.1.2",
    "commander": "^2.9.0",
    "fs-extra": "^0.26.5",
    "lodash": "^4.2.0",
    "nightmare": "^2.1.6",
    "prompt": "^0.2.14",
    "request": "^2.69.0"
  },
  "coffeelintConfig": {
    "max_line_length": {
      "name": "max_line_length",
      "value": 120,
      "level": "error",
      "limitComments": true
    }
  }
}
